---
import type { ProjectFrontmatter } from "../../content/projects";
import { GLOBAL } from "../../lib/variables";
import Featured from "../Featured.astro";
import ProjectSnippet from "../projects/ProjectSnippet.astro";

type Props = {
  featuredProjects: Array<ProjectFrontmatter>;
};

const { featuredProjects } = Astro.props;
---

<Featured url="/lab" pageName="lab">
  {
    featuredProjects.map((project) => (
      <li>
        <ProjectSnippet
          title={project.title}
          description={project.description}
          url={project.filename}
          githubUrl={project.githubUrl}
          liveUrl={project.liveUrl}
          tags={project.tags ?? []}
        />
      </li>
    ))
  }
</Featured>
